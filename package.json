{
  "name": "github-community-dark",
  "version": "0.1.0",
  "description": "Dark GitHub Community",
  "license": "CC-BY-SA-4.0",
  "repository": "StylishThemes/GitHub-Community-Dark",
  "homepage": "https://github.com/StylishThemes/GitHub-Community-Dark",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-silverwind": "^1.0.44",
    "perfectionist": "^2.4.0",
    "postcss": "^7.0.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "updates": "^3.2.3"
  },
  "scripts": {
    "eslint": "eslint --quiet --color *.js tools/*.js",
    "stylelint": "stylelint --silent --color -- github-dark.css themes/**/*.css",
    "perfectionist": "perfectionist",
    "test": "npm run eslint && npm run stylelint",
    "update": "updates -cu && npm install"
  }
}
